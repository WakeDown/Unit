<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
    </configSections>
    <connectionStrings>
        <add name="UnitDashboard.Properties.Settings.DBOptionsConnectionString"
            connectionString="Data Source=|DataDirectory|\DBOptions.sdf"
            providerName="Microsoft.SqlServerCe.Client.4.0" />
        <add name="UnitDashboard.Properties.Settings.Database1ConnectionString"
            connectionString="Data Source=|DataDirectory|\Database1.sdf"
            providerName="Microsoft.SqlServerCe.Client.4.0" />
        <add name="UnitDashboard.Properties.Settings.Database2ConnectionString"
            connectionString="Data Source=|DataDirectory|\Database2.sdf"
            providerName="Microsoft.SqlServerCe.Client.4.0" />
        <add name="UnitDashboard.Properties.Settings.Database1ConnectionString1"
            connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\Database1.mdf;Integrated Security=True"
            providerName="System.Data.SqlClient" />
        <add name="UnitDashboard.Properties.Settings.DBServiceOptionsConnectionString"
            connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\DBServiceOptions.mdf;Integrated Security=True"
            providerName="System.Data.SqlClient" />
        <add name="UnitDashboard.Properties.Settings.ServiceOptionsConnectionString"
            connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\ServiceOptions.mdf;Integrated Security=True"
            providerName="System.Data.SqlClient" />
        <add name="UnitDashboard.Properties.Settings.DBPageOptionsConnectionString"
            connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\Data\DBPageOptions.mdf;Integrated Security=True"
            providerName="System.Data.SqlClient" />
        <add name="UnitDashboard.Properties.Settings.Database1ConnectionString2"
            connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\Data\Database1.mdf;Integrated Security=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="ExchangeRatesSoap" />
                <binding name="GlobalWeatherSoap" />
            </basicHttpBinding>
            <customBinding>
                <binding name="ExchangeRatesSoap12">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
                <binding name="GlobalWeatherSoap12">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://webservices.lb.lt/ExchangeRates/ExchangeRates.asmx"
                binding="basicHttpBinding" bindingConfiguration="ExchangeRatesSoap"
                contract="ExchangeRatesServiceReference.ExchangeRatesSoap"
                name="ExchangeRatesSoap" />
            <endpoint address="http://webservices.lb.lt/ExchangeRates/ExchangeRates.asmx"
                binding="customBinding" bindingConfiguration="ExchangeRatesSoap12"
                contract="ExchangeRatesServiceReference.ExchangeRatesSoap"
                name="ExchangeRatesSoap12" />
            <endpoint address="http://www.webservicex.com/globalweather.asmx"
                binding="basicHttpBinding" bindingConfiguration="GlobalWeatherSoap"
                contract="WeatherServiceReference.GlobalWeatherSoap" name="GlobalWeatherSoap" />
            <endpoint address="http://www.webservicex.com/globalweather.asmx"
                binding="customBinding" bindingConfiguration="GlobalWeatherSoap12"
                contract="WeatherServiceReference.GlobalWeatherSoap" name="GlobalWeatherSoap12" />
        </client>
    </system.serviceModel>
</configuration>